  "main": "index.js",
  "scripts": {
    "build": "babel src -d dist/src",
    "example": "npm run build && node dist/src/index.js",
    "start": "npm run build --watch && npm run dev",
    "start:fork": "node dist/src/index.js --puerto=8081",
    "start:cluster": "node dist/src/index.js --puerto=8082 --cluster",
    "start:inspect": "node --inspect dist/src/index.js",
    "start:profiling": "node --prof dist/src/index.js",
    "serve": "babel-node index.js",
    "prod": "pm2 start ./src/config/ecosystem.config.js",
    "dev": "npm run build && pm2 start dist/src/index.js",
    "test": "jest",
    "rollback:db": "npx knex migrate:rollback --env development",
    "migrate:db": "npx knex migrate:latest --env development",
    "seed:db": "npx knex seed:run --env development",
    "artillery-quick:cluster": "artillery quick --count 50 -n 40 http://localhost:8081/api/processInfo",
    "artillery-quick:fork": "artillery quick --count 50 -n 40 http://localhost:8082/api/processInfo",
    "artillery:fork": "artillery run src/config/artillery/testFork.yml",
    "artillery:cluster": "artillery run src/config/artillery/testCluster.yml"
  },
